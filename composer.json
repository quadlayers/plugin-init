{
	"name": "quadlayers/plugin-init",
	"description": "Plugin Init",
	"type": "project",
	"authors": [
		{
			"name": "QuadLayers",
			"email": "noreply@quadlayers.com"
		}
	],
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"automattic/jetpack-composer-plugin": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"autoload": {
		"psr-4": {
			"QuadLayers\\PluginInit\\": "lib/"
		}
	},
	"require": {
		"automattic/jetpack-composer-plugin": "*",
		"automattic/jetpack-assets": "*",
		"quadlayers/wp-dashboard-widget-news": "^1.0.0",
		"quadlayers/wp-notice-plugin-required": "^2.0.0",
		"quadlayers/wp-plugin-table-links": "^1.0.0"
	},
	"extra": {
		"wp-plugin-slug": "plugin-init"
	},
	"require-dev": {
		"phpunit/phpunit": "9",
		"dealerdirect/phpcodesniffer-composer-installer": "^1.0",
		"wp-coding-standards/wpcs": "^2.3"
	},
	"scripts": {
		"test:phpunit": "./vendor/bin/phpunit phpunit"
	}
}

{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:@wordpress/eslint-plugin/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/prop-types": "off",
		"jsdoc/check-tag-names": "off",
		"jsx-a11y/label-has-associated-control": "off",
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		]
	}
}
